<h2 class="text-center">My_Page</h2>
<div class="container">
  <div class="row">
    <div class="col-md-6 col-ms-6">
      <%= image_tag @contributor.get_profile_image(300,300) %>
    </div>
    <div class="col-md-6 col-ms-6">
      <table>
        <%= form_with model: @contributor, url:post_admin_contributor_path, method: :patch do |f| %>
        <tr>
          <th>氏名:<%= f.text_field :last_name %> <%= f.text_field  :first_name %></th>
        </tr>
        <tr>
          <th>カナ:<%= f.text_field :last_name_kana %> <%= f.text_field :first_name_kana %></th>
        <tr>
          <th>ブランド名:<%= f.text_field :brand_name %></th>
        </tr>
        <tr>
          <th>ブランド画像:<%= f.file_field :profile_image, accept: "image/*" %></th>
        </tr>
        <tr>
          <th>E-mail:<%= f.text_field :email %></th>
        </tr>
        <tr>
          <th>郵便番号:<%= f.text_field :postal_code %></th>
        <tr>
        <tr>
          <th>住所:<%= f.text_field :address %></th>
        </tr>
        <tr>
          <th>電話番号:<%= f.text_field :telephone_number %></th>
        </tr>
        <tr>
          <th><%= f.submit"Keep"%><%= link_to "Withdrawal",post_admin_unsubscribe_path %></th>
        </tr>
        <% end %>
      </table>
    </div>
  </div>
</div>
