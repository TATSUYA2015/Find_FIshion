<h2 class="text-center">My_Page</h2>
<div class="my-page">
  <div class="container">
    <div class="row">
      <div class="col-md-6 col-ms-6">
        <%= image_tag @contributor.get_profile_image(300,300) %>
      </div>
      <div class="col-md-6 col-ms-6">
        <%= form_with model: @contributor, url: post_admin_contributor_path, method: :patch do |f| %>
          <ul>
            <li>
              <label>Name:</label>
              <%= f.text_field :last_name %> <%= f.text_field  :first_name %>
            </li>
            <li>
              <label>Kana:</label>
              <%= f.text_field :last_name_kana %> <%= f.text_field :first_name_kana %>
            </li>
            <li>
              <label>Brand_Name:</label>
              <%= f.text_field :brand_name %>
            </li>
            <li>
              <label>Brand_Image:</label>
              <%= f.file_field :profile_image, accept: "image/*" %>
            </li>
            <li>
              <label>E-mail:</label>
              <%= f.text_field :email %>
            </li>
            <li>
              <label>〒</label>
              <%= f.text_field :postal_code %>
            </li>
            <li>
               <label>Address:</label>
               <%= f.text_field :address %>
            </li>
            <li>
              <label>Tel:</label>
              <%= f.text_field :telephone_number %>
            </li>
            <li>
               <label></label>
               <%= f.text_area :introduction, placeholder: "ブランド説明" %>
            </li>
            <li>
             <label><%= f.submit"Keep",class: "btn btn-outline-secondary mr-3vw"%></label>
             <%= link_to "Withdrawal",post_admin_unsubscribe_path, class: "btn btn-outline-secondary mr-3vw" %>
            </li>
          </ul>
        <% end %>
      </div>
    </div>
  </div>
</div>