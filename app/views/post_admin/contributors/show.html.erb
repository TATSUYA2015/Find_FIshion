<h2 class="text-center">My_Page</h2>
<div class="my-page">
  <div class="container">
    <div class="row">
      <div class="col-md-6 col-ms-6">
        <%= image_tag @contributor.get_profile_image(300,300) %>
      </div>
      <div class="col-md-6 col-ms-6">
        <ul>
          <li>
            <label>Name:</label>
            <%= @contributor.last_name %> <%= @contributor.first_name %>
          </li>
          <li>
            <label>Kana:</label>
            <%= @contributor.last_name_kana %> <%= @contributor.first_name_kana %>
          </li>
          <li>
            <label>Brand_Name:</label>
            <%= @contributor.brand_name %>
          </li>
          <li>
            <label>E-mail:</label>
            <%= @contributor.email %>
          </li>
          <li>
            <label>ã€’</label>
            <%= @contributor.postal_code %>
          </li>
          <li>
             <label>Address:</label>
             <%= @contributor.address %>
          </li>
          <li>
            <label>Tel:</label>
            <%= @contributor.telephone_number %>
          </li>
          <li>
             <label></label>
             <%= @contributor.introduction %>
          </li>
          <li>
             <label><%= link_to "Edit",edit_post_admin_contributor_path(current_contributor.id), class: "btn btn-outline-secondary mr-3vw" %></label>
          </li>
        </ul>
      </div>
    </div>
  </div>
</div>