<h1><%= current_contributor.brand_name %>ブランド トップ</h1>
<p><%= link_to "+",new_post_admin_item_path %></p>
<div>
    <% @items.each do |item| %>
      <%= link_to post_admin_item_path(item.id) do %>
        <%= item.name %></br>
        <%= image_tag item.image %></br>
        <%= item.introduction %></br>
        <% end %>
    <% end %>
</div>
