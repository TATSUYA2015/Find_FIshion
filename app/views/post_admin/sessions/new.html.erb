<h2 class="text-center">投稿者Log in</h2>
<br>
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="text-center">
    <%= f.email_field :email, placeholder: "E-mail", autofocus: true, autocomplete: "email" %>
  </div>
  <br>
  <div class="text-center">
    <%= f.password_field :password, placeholder: "Password", autocomplete: "current-password" %>
  </div>
  <br>
    <!--<%# if devise_mapping.rememberable? %>-->
    <!--  <div class="text-center">-->
    <!--    <%#= f.check_box :remember_me %>-->
    <!--    <%#= f.label :remember_me %>-->
    <!--  </div>-->
    <!--<%# end %>-->
  <div class="text-center">
    <%= f.submit "Log in", class: "btn btn-outline-secondary mr-3vw" %>
    <div class="btn btn-outline-secondary mr-3vw ">
      <%= render "post_admin/shared/links" %>
    </div>
  </div>
<% end %>
