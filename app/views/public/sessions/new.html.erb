  <!--    <%#= f.check_box :remember_me %>-->
  <!--    <%#= f.label :remember_me %>-->
  <!--  </div>-->
  <!--<%# end %>-->
  </br>
  <div class="text-center">
    <%= f.submit "Log in", class: "btn btn-outline-secondary mr-3vw" %>
    <div class="btn btn-outline-secondary mr-3vw ">
      <%= render "public/shared/links" %>
    </div>
  </div>
<% end %>
