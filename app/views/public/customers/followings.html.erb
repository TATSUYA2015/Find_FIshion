  <link rel="stylesheets" href="public/followings.scss"/>
  <div class="background">
  <h2 class="text-center">フォロー一覧</h2>
  <div class="following-area">
    <div class="followings">
    <% @contributors.each do |contributor| %>
     <div class="card shadow">
        <%= link_to contributor_path(contributor.id) do %> <!--各ブランド情報ページのリンク -->
          <%= image_tag contributor.get_profile_image(162,211) %></br>
          <%= contributor.brand_name %></br>
        <% end %>
     </div>
    <% end %>
  </div>