<link rel="stylesheets" href="public/favorites.scss"/>
<div class="background">
  <h2 class="text-center">お気に入り一覧</h2>
  <div class="favorite-area">
    <div class="favorites">
    <% @favorite_items.each do |item|%>
      <div class="item-box1">
        <%= link_to item_path(item) do %>
          <%= image_tag item.image ,size:'162x211' %>
        <% end %>
      </div>
    <% end %>
    </div>
  </div>
</div>
